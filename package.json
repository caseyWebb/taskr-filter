{
  "name": "taskr-filter",
  "version": "0.0.1",
  "description": "Filter plugin for Taskr",
  "license": "WTFPL",
  "repository": "https://github.com/caseyWebb/taskr-filter",
  "files": [
    "index.js"
  ],
  "keywords": [
    "taskr",
    "taskr-plugin",
    "filter"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore ./",
    "prepublish": "npm test",
    "test": "jest",
    "posttest": "rm -rf test/.tmp"
  },
  "author": {
    "name": "Casey Webb",
    "email": "notcaseywebb@gmail.com",
    "url": "https://caseyWebb.xyz"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "multimatch": "^4.0.0"
  },
  "devDependencies": {
    "@taskr/clear": "^1.1.0",
    "eslint": "^4.1.0",
    "eslint-config-profiscience": "^2.0.0",
    "jest": "^24.9.0",
    "taskr": "^1.1.0"
  },
  "peerDependencies": {
    "taskr": "^1.1.0"
  },
  "engines": {
    "node": ">= 8.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-profiscience"
    ],
    "env": {
      "jest": true
    },
    "parserOptions": {
      "sourceType": "script"
    }
  },
  "jest": {
    "collectCoverage": true,
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "semi": false,
    "singleQuote": true
  }
}
